@model EventServer.Models.HomeIndexModel
@{
    ViewBag.Title = "Home Page";
}
<div id="content">
    @{ Html.RenderAction("HomeMessage", "Home"); }
    @Html.Partial("Posts", Model.Posts)
</div>
<div id="sidebar">
    @{ Html.RenderAction("AdminMenuWidget", "Admin"); }
    @{ Html.RenderAction("Sidebar", "Home"); }
    @Html.Partial("Tweets", Model.Tweets)
</div>
